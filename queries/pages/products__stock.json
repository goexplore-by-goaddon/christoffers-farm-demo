{
  "companies": {
    "klass": "companies",
    "query": {
      "token": "{{ token }}"
    },
    "projection": {
      "_id": 1
    }
  },
  "products": {
    "klass": "products",
    "query": {
      "sku": "{{ sku }}",
      "company_id": "{{ company_id }}"
    },
    "projection": {
      "_id": 1
    }
  }
}