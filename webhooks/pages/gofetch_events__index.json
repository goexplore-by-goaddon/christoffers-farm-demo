{
  "print_order": {
    "method": "post",
    "url": "https://app.shipmondo.com/api/public/v3/shipments",
    "headers": {
      "Authorization": "{{ shipmondo_api_token }}",
      "Content-Type": "application/json"
    },
    "body": {
      "test_mode": false,
      "own_agreement": false,
      "product_code": "BRI_PP",
      "service_codes": "EMAIL_NT",
      "automatic_select_service_point": true,
      "sender": {
        "name": "Christoffers Farm",
        "address1": "Vigerstedvej 8",
        "zipcode": "4100",
        "city": "Ringsted",
        "country_code": "DK"
      },
      "receiver": {
        "name": "{{ name }}",
        "address1": "{{ address_1 }}",
        "address2": "{{ address_2 }}",
        "zipcode": "{{ zip_code }}",
        "city": "{{ city }}",
        "country_code": "DK",
        "email": "{{ mail_address }}",
        "mobile": "{{ phone }}"
      },
      "parcels": [
        {
          "weight": "{{ package_weight | int }}"
        }
      ],
      "print": true,
      "reference": "{{ packing_list }}"
    }
  }
}