{
  "update_variant": {
    "method": "patch",
    "url": "https://{{ gofetch_url }}/shop_api/v1/variants/{{ variant_id }}/change_stock",
    "headers": {
      "Content-Type": "application/json",
      "Authorization": "{{ gofetch_shop_api_token }}"
    },
    "body": {
      "data": {
        "stock_change": "{{ stock_change }}"
      }
    }
  }
}